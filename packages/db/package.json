{
    "name": "@zodive/db",
    "version": "0.0.0",
    "main": "./index.ts",
    "types": "./index.ts",
    "scripts": {
        "db:generate": "pnpm with-env prisma generate",
        "db:push": "pnpm with-env prisma db push --skip-generate",
        "db:migrate": "pnpm with-env prisma migrate dev",
        "with-env": "dotenv -e ../../.env --"
    },
    "dependencies": {
        "@prisma/client": "^4.12.0",
        "@zodive/env": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "18.15.11",
        "@zodive/config": "workspace:*",
        "dotenv-cli": "^7.2.1",
        "prisma": "^4.12.0",
        "typescript": "5.0.4"
    }
}
